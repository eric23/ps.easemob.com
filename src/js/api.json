{
  "token": "",
  "servers": [
    {
      "name": "https://a1.sdb.easemob.com"
    },
    {
      "name": "https://a1.easemob.com"
    }
  ],
  "services": [
    {
      "group": "callback",
      "apis": [
        {
          "name": "callback_get",
          "uri": "/:org/:app/callbacks",
          "method": "GET",
          "auth": true
        },
        {
          "name": "callback_add",
          "uri": "/:org/:app/callbacks",
          "method": "POST",
          "auth": true,
          "body": [
            {
              "property": "name",
              "dtype": "text",
              "required": true,
              "tip": "不可重复，建议用有含义的字符串命名"
            },
            {
              "property": "msgTypes",
              "dtype": "enum",
              "options": [
                "chat",
                "chat_offline"
              ],
              "required": true
            },
            {
              "property": "hxSecret",
              "dtype": "text",
              "required": false
            },
            {
              "property": "secret",
              "dtype": "text",
              "required": false
            },
            {
              "property": "targetUrl",
              "dtype": "url",
              "required": false
            },
            {
              "property": "status",
              "dtype": "enum",
              "options": [
                0,
                1,
                2
              ],
              "required": false,
              "help": "开通请选 1"
            },
            {
              "property": "notifyUrl",
              "dtype": "url",
              "required": false
            }
          ]
        }
      ]
    }, 
    {
      "group": "ops",
      "apis": [
        {
          "name": "enable_service",
          "uri": "/ops/payedProduct/addPayedProduct",
          "method": "POST",
          "auth": true,
          "body": [
            {
              "property": "appkey",
              "dtype": "text",
              "required": true
            },
            {
              "property": "productType",
              "dtype": "text",
              "required": true
            },
            {
              "property": "startTime",
              "dtype": "date",
              "required": true
            },
            {
              "property": "dateType",
              "dtype": "enum",
              "options": [
                "DAY",
                "MONTH",
                "YEAR"
              ],
              "required": true
            },
            {
              "property": "openTime",
              "dtype": "number",
              "required": true
            },
            {
              "property": "status",
              "dtype": "text",
              "required": true
            },
            {
              "property": "operator",
              "dtype": "text",
              "required": true
            }
          ]
        }
      ]
    }
  ]
}